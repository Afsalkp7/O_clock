<!doctype html>
<html lang="en">

<head>
    <title>Sidebar 05</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/style2.css">
    <!-- Latest compiled and minified CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body>
    <section>
        <div class="wrapper d-flex align-items-stretch">
            <nav id="sidebar">
                <div class="custom-menu">
                    <button type="button" id="sidebarCollapse" class="btn btn-primary">
                        <i class="fa fa-bars"></i>
                        <span class="sr-only">Toggle Menu</span>
                    </button>
                </div>
                <div class="p-4">
                    <h1><a href="" class="logo">O'Clock<span> {{admin.name}}</span></a></h1>
                    <ul class="list-unstyled components mb-5">
                        <li class="active">
                            <a href="#"><span class="fa fa-home mr-3"></span> Home</a>
                        </li>
                        <li>
                            <a href="/profile" data-bs-toggle="modal" data-bs-target="#staticBackdrop"><span
                                    class="fa fa-user mr-3"></span>Profile</a>
                        </li>
                        <li>
                            <a href="#"><span class="fa-solid fa-users mr-3"></span>User</a>
                        </li>
                        <li>
                            <a href="#"><span class="fa-solid fa-cart-shopping mr-3"></span>Order</a>
                        </li>
                        <li>
                            <a href="#"><span class="fa fa-sticky-note mr-3"></span>Category</a>
                        </li>
                        <li>
                            <a href="#"><span class="fa fa-suitcase mr-3"></span> Products</a>
                        </li>
                        <li>
                            <a href="#"><span class="fa fa-cogs mr-3"></span> Brand</a>
                        </li>
                        <li>
                            <a href="#"><span class="fa fa-paper-plane mr-3"></span> Banner</a>
                        </li>
                        <li>
                            <a href="#"><span class="fa-solid fa-message mr-3"></span>Messages</a>
                        </li>
                        <li>
                            <a href="#"><span class="fa-solid fa-star mr-3"></span>Reviews</a>
                        </li>
                    </ul>

                    <div class="mb-4">
                        <a href="#" id="logout"><span class="fa-solid fa-right-from-bracket mr-3"></span>Logout</a>

                    </div>
                </div>
            </nav>
    </section>

    <!-- Page Content  -->
    <div id="content" class="p-4 p-md-5 pt-5">
        <section class="container">
            <div class="container mt-5 shadow-lg P-5 text-center"
                style="width: 30%; background:linear-gradient(rgb(176, 176, 237),lightblue);">
                <h1 style="color: red; font-weight:bolder">Welcome </h1>
                <h2>{{admin.name}}</h2>
                <h3>{{admin.email}}</h3>
                <h3>{{admin.phone}}</h3>
            </div>

            <div class="container shadow-lg P-5 text-center"
                style="width: 30%; background:linear-gradient(rgb(176, 176, 237),lightblue);">
                <a href="/update"><button type="button" class="btn btn-dark">Edit</button></a>
                <a href="/logout"><button type="button" class="btn btn-dark">LogOut</button></a>

            </div>

        </section>


    </div>





    <script src="/js/jquery.min.js"></script>
    <script src="/js/popper.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/main.js"></script>
</body>

</html>