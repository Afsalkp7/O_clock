                    <a class="navbar-brand text-dark" href="/" id="logo_img">O|o' CLOCK</a>

                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false"
                        aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
                        <ul class="navbar-nav ms-auto mb-2 mb-lg-0" id="navitems">
                            <li class="nav-item">
                                <a class="nav-link active" aria-current="page" href="/">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="/shop">Shop</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="/contact">Contact</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="/about">About Us</a>
                            </li>
                            

                        </ul>
                        <ul class="navbar-nav ms-auto mb-2 mb-lg-0" id="navicons">
                            <li class="nav-item">
                                <a class="nav-link" href="/user"><i class="fa-solid fa-user"></i><span
                                        style="color: black; font-family:sanserif; font-weight:800" class="ps-2">{{user.name}}</span></a>

                            </li>
                           
                            <li class="nav-item">
                                <a class="nav-link" href="/wish"><i class="fa-regular fa-heart"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/cart"><i class="fa-solid fa-cart-shopping"></i></a>
                            </li>

                        </ul>
                    </div>
                </div>
            </nav>

        </div>
    </section>
    {{! navsection end }}

    

    <section class="" style="margin-top: 70px;">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12 col-sm-12 col-md-4 col-lg-3 col-xl-3 col-xxl-3 p-3">
                    <p style="font-weight: 700;">CATEGORIES</p>
                    <ul style="list-style: none;">
                        {{!-- <a href="">
                            <li style="color: black; font-weight: 600" id="allCategories">All</li>
                        </a> --}}
                        {{#each category}}
                        <li>
                            <input class="filter_category" type="checkbox" value="{{this.name}}" id="category">
                            <label class="category-name" for="category-name" >{{this.name}}</label>

                        </li>
                        {{/each}}
                    </ul>
                    <p style="font-weight: 700;">BRANDS</p>
                    <ul style="list-style: none;">
                        {{!-- <a href="">
                            <li style="color: black; font-weight: 600">All</li>
                        </a> --}}
                        {{#each brands}}
                        <li>
                            <input class="filter_brand" type="checkbox" value="{{this.name}}" id="brand-{{this.name}}">
                            <label class="brand" for="brand-{{this.name}}">{{this.name}}</label>

                        </li>
                        {{/each}}
                    </ul>
                    <div>
                        <p style="font-weight: 700;">PRICE RANGE</p>
                        <section class="range-slider">
                        <span class="rangeValues"></span>
                        <input value="0" min="0" max="28000" step="500" type="range">
                        <input value="28000" min="0" max="28000" step="500" type="range">
                        </section>
                    </div>
                    
                </div>
                <div class="col-12 col-sm-12 col-md-8 col-lg-9 col-xl-9 col-xxl-9 ">
                    <div class="container">
                        <div class="row">
                            {{#each products}}
                            <div class="col-12 col-sm-12 col-md-6 col-lg-6 col-xl-3 col-xxl-3 p-2 pro"
                                data-category="{{this.category}}" data-brand="{{this.brand}}"
                                data-price="{{this.price}}">
                                <a href="/shop/{{this._id}}">
                                    <div class="card  shadow-lg">
                                        <img src="{{this.images}}" class="card-img-top" alt="{{this.name}}"
                                            data-product-id="{{this._id}}">
                                        <div class="card-body text-center">
                                            <h5 class="card-title text-secondary" style="font-size: 17px;">
                                                {{this.brand}}
                                            </h5>
                                            <span class="card-text">{{this.name}}</span>
                                            <p class="card-text1">Rs: {{this.price}}/-</p>
                                        </div>
                                    </div>
                                </a>
                            </div>
                            {{/each}}
                        </div>

                    </div>

                </div>
            </div>
        </div>

    </section>


 {{!-- footer sec start --}}

